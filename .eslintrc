{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
//    "indent": [2, 4],
    "no-console": 0,
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "ignore"
    }
    ]
  }
}